            <androidx.recyclerview.widget.RecyclerView
                android:layout_marginTop="@dimen/value_10dp"
                android:id="@+id/imageRecyclerViewRes"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginStart="@dimen/value_10dp"
                android:layout_marginEnd="@dimen/value_10dp"
                android:orientation="horizontal" />


        </LinearLayout>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:background="@color/light_blue_background">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Comments:"
                android:textColor="@color/black"
                android:textStyle="bold"
                android:layout_marginTop="@dimen/text_10sp"
                android:layout_marginStart="@dimen/text_10sp" />

            <TextView
                android:layout_marginStart="@dimen/value_10dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textColor="@color/black"
                android:layout_marginTop="@dimen/five5_sp"
                android:text="Some of the Uploaded photos are not in a \nquality or visible. Re-shoot and upload again"/>



            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="- Apollo Quality Control"
                android:textColor="@color/black"
                android:textStyle="bold"
                android:layout_marginTop="@dimen/five5_sp"
                android:layout_marginStart="@dimen/text_10sp" />

        </LinearLayout>

    </LinearLayout>


    <LinearLayout
        android:visibility="gone"
        android:id="@+id/reshootButton"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:background="#a6a6a6"
        android:orientation="horizontal"
        android:padding="@dimen/value_12dp">

        <TextView
            android:id="@+id/count_of_iages"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/text_15sp"
            android:textStyle="bold"
            android:textColor="@color/white"
            android:textSize="@dimen/text_17sp" />

        <TextView
            android:id="@+id/re_shoot_button"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/value_10dp"
            android:gravity="center"
            android:text="RE-SHOOT AGAIN"
            android:textStyle="bold"
            android:textColor="@color/white"
            android:textSize="@dimen/text_17sp" />


    </LinearLayout>

</RelativeLayout>
</layout>

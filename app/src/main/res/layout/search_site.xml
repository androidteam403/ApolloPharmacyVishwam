<?xml version="1.0" encoding="utf-8"?>

<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">


    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:visibility="visible"
                android:id="@+id/parentLayout"
                android:orientation="vertical">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="#6BBD99"
                    android:orientation="horizontal"
                    android:visibility="visible"
                    android:weightSum="3">


                    <TableLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"

                        android:background="@drawable/dashquarebox_right"

                        android:orientation="horizontal">


                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/value_5dp"
                            android:fontFamily="@font/roboto_medium"
                            android:paddingStart="4sp"
                            android:paddingTop="8dp"
                            android:paddingBottom="8dp"
                            android:text="Executive"
                            android:textAlignment="center"
                            android:textColor="@color/black"
                            android:textSize="@dimen/dimen11sp" />

                    </TableLayout>

                    <TableLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1.017"

                        android:background="@drawable/site_dashquarebox_right"


                        android:orientation="vertical">


                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/value_5dp"
                            android:fontFamily="@font/roboto_medium"
                            android:paddingStart="4sp"
                            android:paddingTop="8dp"
                            android:paddingBottom="8dp"

                            android:text="Manager"
                            android:textAlignment="center"

                            android:textColor="@color/black"
                            android:textSize="@dimen/dimen11sp" />

                    </TableLayout>

                    <TableLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:background="@drawable/dashquarebox_right"

                        android:orientation="horizontal">


                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/value_5dp"
                            android:fontFamily="@font/roboto_medium"
                            android:paddingStart="4sp"
                            android:paddingTop="8dp"
                            android:paddingBottom="8dp"
                            android:text="GM"
                            android:textAlignment="center"
                            android:textColor="@color/black"
                            android:textSize="@dimen/dimen11sp" />

                    </TableLayout>


                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/whitependencysquareboxright"
                    android:orientation="horizontal"

                    android:visibility="visible"
                    android:weightSum="3">


                    <TableLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"

                        android:background="@drawable/dashquarebox_right"

                        android:orientation="horizontal">


                        <TextView
                            android:id="@+id/executiveemp_id"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/value_5dp"
                            android:fontFamily="@font/roboto_medium"
                            android:paddingStart="4sp"
                            android:paddingTop="8dp"
                            android:paddingBottom="8dp"
                            android:text="-"
                            android:textAlignment="center"
                            android:textColor="@color/black"
                            android:textSize="@dimen/dimen11sp" />

                    </TableLayout>

                    <TableLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1.017"

                        android:background="@drawable/site_dashquarebox_right"


                        android:orientation="vertical">


                        <TextView
                            android:id="@+id/manageremp_id"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/value_5dp"
                            android:fontFamily="@font/roboto_medium"
                            android:paddingStart="4sp"
                            android:paddingTop="8dp"
                            android:paddingBottom="8dp"

                            android:text="-"
                            android:textAlignment="center"

                            android:textColor="@color/black"
                            android:textSize="@dimen/dimen11sp" />

                    </TableLayout>

                    <TableLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:background="@drawable/dashquarebox_right"

                        android:orientation="horizontal">


                        <TextView
                            android:id="@+id/gmanageremp_id"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/value_5dp"
                            android:fontFamily="@font/roboto_medium"
                            android:paddingStart="4sp"
                            android:paddingTop="8dp"
                            android:paddingBottom="8dp"
                            android:text="-"

                            android:textAlignment="center"
                            android:textColor="@color/black"
                            android:textSize="@dimen/dimen11sp" />

                    </TableLayout>


                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/searchsitependencysquareboxright"
                    android:orientation="horizontal"
                    android:visibility="visible"
                    android:weightSum="4">


                    <TableLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"

                        android:background="@drawable/dashquarebox_right"

                        android:orientation="horizontal">


                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/value_5dp"
                            android:fontFamily="@font/roboto_medium"
                            android:paddingStart="4sp"
                            android:paddingTop="8dp"
                            android:paddingBottom="8dp"
                            android:text="RTO"
                            android:textAlignment="center"
                            android:textColor="@color/black"
                            android:textSize="@dimen/dimen11sp" />

                    </TableLayout>

                    <TableLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1.017"

                        android:background="@drawable/site_dashquarebox_right"


                        android:orientation="vertical">


                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/value_5dp"
                            android:fontFamily="@font/roboto_medium"
                            android:paddingStart="4sp"
                            android:paddingTop="8dp"
                            android:paddingBottom="8dp"

                            android:text="RTO Value"
                            android:textAlignment="center"

                            android:textColor="@color/black"
                            android:textSize="@dimen/dimen11sp" />

                    </TableLayout>

                    <TableLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:background="@drawable/dashquarebox_right"

                        android:orientation="horizontal">


                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/value_5dp"
                            android:fontFamily="@font/roboto_medium"
                            android:paddingStart="4sp"
                            android:paddingTop="8dp"
                            android:paddingBottom="8dp"
                            android:text="RRTO"
                            android:textAlignment="center"
                            android:textColor="@color/black"
                            android:textSize="@dimen/dimen11sp" />

                    </TableLayout>

                    <TableLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"

                        android:background="@drawable/site_dashquarebox_right"


                        android:orientation="vertical">


                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/value_5dp"
                            android:fontFamily="@font/roboto_medium"
                            android:paddingStart="4sp"
                            android:paddingTop="8dp"
                            android:paddingBottom="8dp"

                            android:text="RRTO Value"
                            android:textAlignment="center"

                            android:textColor="@color/black"
                            android:textSize="@dimen/dimen11sp" />

                    </TableLayout>


                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/white"
                    android:orientation="horizontal"
                    android:visibility="visible">


                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/squareboxright"
                        android:orientation="horizontal"

                        android:weightSum="4">


                        <TableLayout
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:background="@drawable/dashquarebox_right"

                            android:orientation="vertical">


                            <TextView
                                android:id="@+id/rtocount"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/value_5dp"
                                android:fontFamily="@font/roboto_medium"
                                android:paddingStart="@dimen/value_25dp"

                                android:paddingTop="8dp"

                                android:paddingBottom="8dp"
                                android:text="-"
                                android:textAlignment="center"
                                android:textColor="@color/black"
                                android:textSize="@dimen/dimen11sp" />

                        </TableLayout>

                        <TableLayout
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1.017"

                            android:background="@drawable/site_dashquarebox_right"


                            android:orientation="vertical">


                            <TextView
                                android:id="@+id/rtovalue"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/value_5dp"
                                android:drawableLeft="@drawable/qc_ic_rupee_indian"
                                android:drawablePadding="2dp"
                                android:fontFamily="@font/roboto_medium"
                                android:paddingStart="@dimen/value_25dp"
                                android:paddingTop="8dp"
                                android:paddingBottom="8dp"
                                android:text="-"

                                android:textAlignment="textStart"
                                android:textColor="@color/black"
                                android:textSize="@dimen/dimen11sp" />

                        </TableLayout>

                        <TableLayout
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:background="@drawable/dashquarebox_right"

                            android:orientation="vertical">


                            <TextView
                                android:id="@+id/rrtocount"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/value_5dp"
                                android:fontFamily="@font/roboto_medium"
                                android:paddingTop="8dp"
                                android:paddingBottom="8dp"
                                android:text="-"

                                android:textAlignment="center"
                                android:textColor="@color/black"
                                android:textSize="@dimen/dimen11sp" />

                        </TableLayout>

                        <TableLayout
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"

                            android:background="@drawable/site_dashquarebox_right"


                            android:orientation="vertical">


                            <TextView
                                android:id="@+id/rrtovalue"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/value_5dp"
                                android:drawableLeft="@drawable/qc_ic_rupee_indian"
                                android:drawablePadding="2dp"
                                android:fontFamily="@font/roboto_medium"
                                android:paddingStart="@dimen/value_25dp"
                                android:paddingTop="8dp"
                                android:paddingBottom="8dp"
                                android:text="-"
                                android:textAlignment="textStart"

                                android:textColor="@color/black"                            android:textSize="@dimen/dimen11sp" />

                        </TableLayout>

                        <TableLayout
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:background="@drawable/dashquarebox_right"
                            android:orientation="vertical"
                            android:visibility="gone">


                            <TextView
                                android:id="@+id/pending_count"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/value_5dp"
                                android:fontFamily="@font/roboto_medium"
                                android:paddingStart="4sp"
                                android:paddingTop="8dp"
                                android:paddingBottom="8dp"
                                android:text="-"
                                android:textAlignment="center"
                                android:textColor="@color/black"
                                android:textSize="@dimen/text_10sp" />

                        </TableLayout>


                        <TableLayout
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:background="@drawable/squarebox_right"

                            android:orientation="vertical"

                            android:visibility="gone">


                            <TextView
                                android:id="@+id/manager"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/value_5dp"
                                android:fontFamily="@font/roboto_bold"
                                android:paddingStart="4sp"
                                android:paddingTop="8dp"
                                android:paddingBottom="8dp"
                                android:text="-"

                                android:textAlignment="center"
                                android:textColor="#40a776"

                                android:textSize="@dimen/text_10sp" />
                        </TableLayout>

                        <TableLayout
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:background="@drawable/squarebox"
                            android:orientation="vertical"

                            android:visibility="gone">


                            <TextView
                                android:id="@+id/generalmanager"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/value_5dp"
                                android:fontFamily="@font/roboto_bold"
                                android:paddingStart="8dp"
                                android:paddingTop="8dp"
                                android:paddingBottom="8dp"
                                android:text="-"

                                android:textAlignment="center"
                                android:textColor="#663399"

                                android:textSize="12sp" />
                        </TableLayout>
                    </LinearLayout>

                </LinearLayout>
            </LinearLayout>



        </LinearLayout>
    </RelativeLayout>

</layout>
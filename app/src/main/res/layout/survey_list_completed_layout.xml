<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/value_2dp"
        android:background="#e8f7f2"
        android:paddingVertical="@dimen/value_10dp"
     >

        <RelativeLayout
            android:visibility="visible"
            android:layout_marginStart="@dimen/value_30dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <RelativeLayout
                    android:id="@+id/header_layout"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content">


                    </LinearLayout>

                    <ImageView
                        android:id="@+id/survey_img"
                        android:layout_width="@dimen/value_50dp"
                        android:layout_height="@dimen/value_50dp"
                        android:layout_alignParentEnd="true"
                        android:layout_marginEnd="@dimen/value_30dp"
                        android:src="@drawable/ic_pending_list_icons_survey3" />

                    <ImageView
                        android:id="@+id/expand"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentEnd="true"
                        android:layout_centerVertical="true"
                        android:layout_marginEnd="@dimen/value_10dp"
                        android:src="@drawable/rightarrow_black"
                        app:tint="@color/black" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">

                            <TextView
                                android:id="@+id/store_id"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textStyle="bold"
                                android:textSize="@dimen/text_13sp"
                                android:text="Store ID: "
                                android:textColor="#76828e" />

                            <TextView
                                android:id="@+id/store_id_text"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_toRightOf="@+id/store_id"
                                android:text="16741"
                                android:textColor="@color/black"
                                android:textSize="@dimen/text_15sp"
                                android:textStyle="bold" />
                        </LinearLayout>
                        <TextView
                            android:id="@+id/location"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/store_id"
                            android:text="Gandhi Nagar, Nellore"
                            android:textColor="@color/black"
                            android:textSize="@dimen/text_15sp"
                            android:textStyle="bold" />
                    </LinearLayout>





                </RelativeLayout>



                <LinearLayout
                    android:layout_marginTop="@dimen/value_5dp"
                    android:id="@+id/completed_status_layout"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:visibility="visible">

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/value_30dp"
                        android:orientation="vertical">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Date of the Visit"
                            android:textColor="#76828e"
                            android:textStyle="bold"
                            android:textSize="@dimen/text_13sp" />

                        <TextView
                            android:id="@+id/date_of_visit"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="20 Dec,2022 - 10:45am"
                            android:textColor="@color/black"
                            android:textSize="@dimen/text_15sp"
                            android:textStyle="bold" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Status"
                            android:textStyle="bold"
                            android:textColor="#76828e"
                            android:textSize="@dimen/text_13sp" />

                        <TextView
                            android:id="@+id/status"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Completed"
                            android:textColor="#70a81f"
                            android:textSize="@dimen/text_15sp"
                            android:textStyle="bold" />
                    </LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_marginTop="@dimen/value_15dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/executive_email"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Executive: "
                        android:textStyle="bold"
                        android:textSize="@dimen/text_13sp"
                        android:textColor="@color/dark_grey_for_heading" />

                    <TextView
                        android:id="@+id/executive_email_text"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="@dimen/text_15sp"
                        android:text="ramesh_p@apollopharmacy.org"
                        android:textColor="#1575a5"
                        android:textStyle="bold" />
                </LinearLayout>

            </LinearLayout>


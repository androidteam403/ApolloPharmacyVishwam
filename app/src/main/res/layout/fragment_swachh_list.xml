<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/value_10dp"
                android:layout_marginTop="@dimen/value_10dp"
                android:layout_marginEnd="@dimen/value_10dp"
                android:gravity="center_vertical"
                android:orientation="horizontal">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <ImageView
                        android:layout_width="@dimen/text_22sp"
                        android:layout_height="@dimen/text_22sp"
                        android:layout_gravity="center"
                        android:src="@drawable/person_1" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/value_8dp"
                        android:orientation="vertical">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="User ID"
                            android:textColor="@color/reg_date_color"
                            android:textSize="@dimen/text_12sp"/>

                        <TextView
                            android:id="@+id/userId_swachlist"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textColor="@color/black"
                            android:textSize="@dimen/text_12sp"
                            android:textStyle="bold" />


                    </LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/text_14sp"
                    android:gravity="center_vertical"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/value_8dp"
                        android:layout_marginTop="@dimen/value_3dp"
                        android:orientation="vertical">


                        <ImageView
                            android:id="@+id/calender_from"
                            android:layout_width="@dimen/text_22sp"
                            android:layout_height="@dimen/text_22sp"
                            android:layout_gravity="center"
                            android:layout_marginTop="@dimen/three3_sp"
                            android:contentDescription="@string/app_name"
                            android:onClick="@{()-> callback.onClickFromDate()}"
                            android:src="@drawable/calendar" />


                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/value_8dp"
                        android:orientation="vertical">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="From"
                            android:textColor="@color/reg_date_color"
                            android:textSize="@dimen/text_12sp" />


                        <TextView
                            android:id="@+id/from_date"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textColor="@color/black"
                            android:textSize="@dimen/text_12sp"
                            android:textStyle="bold" />

                    </LinearLayout>


                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="11dp"
                        android:layout_marginTop="@dimen/value_3dp"
                        android:orientation="vertical">


                        <ImageView
                            android:id="@+id/calender_to"
                            android:layout_width="@dimen/text_22sp"
                            android:layout_height="@dimen/text_22sp"
                            android:layout_gravity="center"
                            android:layout_marginTop="@dimen/three3_sp"
                            android:contentDescription="@string/app_name"
                            android:onClick="@{()-> callback.onClickToDate()}"
                            android:src="@drawable/calendar" />

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/value_8dp"
                        android:orientation="vertical">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="To"
                            android:textColor="@color/reg_date_color"
                            android:textSize="@dimen/text_12sp" />

                        <TextView
                            android:id="@+id/to_date"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:textColor="@color/black"
                            android:textSize="@dimen/text_12sp"
                            android:textStyle="bold" />

                    </LinearLayout>


                </LinearLayout>

                <RelativeLayout
                    android:id="@+id/cameraDelete"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/value_40dp"
                    android:layout_marginStart="@dimen/value_5dp"
                    android:layout_marginTop="@dimen/value_3dp"
                    android:gravity="center_vertical"
                    android:visibility="visible">

                    <ImageView
                        android:layout_width="@dimen/value_25dp"
                        android:layout_height="@dimen/value_25dp"
                        android:layout_marginTop="@dimen/four_4sp"
                        android:contentDescription="@string/app_name"
                        android:onClick="@{()-> callback.onClickSearch()}"
                        android:src="@drawable/ic_search_circle" />

                    <ImageView
                        android:layout_width="@dimen/value_25dp"
                        android:layout_height="@dimen/value_25dp"
                        android:layout_alignParentEnd="true"
                        android:layout_marginStart="@dimen/value_5dp"
                        android:background="@drawable/red_rounded_corners"
                        android:contentDescription="@string/app_name"
                        android:padding="@dimen/value_4dp"
                        android:src="@drawable/trash"
                        android:visibility="gone" />
                </RelativeLayout>


            </LinearLayout>

            <androidx.recyclerview.widget.RecyclerView
                android:id="@+id/storeIdsRecyclerView"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/value_12dp"
                android:layout_marginTop="@dimen/value_5dp"
                android:layout_marginEnd="@dimen/value_5dp"

                android:layout_marginBottom="2dp"
                android:orientation="horizontal"
                android:visibility="visible"
                app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager" />


            <LinearLayout
                android:id="@+id/tabsforexecutive"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/value_7dp"
                android:visibility="visible"
                android:weightSum="1">

                <androidx.appcompat.widget.AppCompatButton

                    android:id="@+id/approvedListButton"
                    android:layout_width="0dp"
                    android:layout_height="48dp"
                    android:layout_weight="0.5"
                    android:background="@color/header_blue"
                    android:onClick="@{()-> callback.onClickApproved()}"
                    android:text="APPROVED"
                    android:textAlignment="center"
                    android:textColor="@color/white_for_both"
                    android:visibility="visible" />

                <View
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:background="@color/black" />


                <androidx.appcompat.widget.AppCompatButton
                    android:id="@+id/pendingListButton"
                    android:layout_width="0dp"
                    android:layout_height="48dp"
                    android:layout_weight="0.5"
                    android:background="@color/grey"
                    android:onClick="@{()-> callback.onClickPending()}"
                    android:text="PENDING"
                    android:textAlignment="center"
                    android:textColor="@color/white_for_both"
                    android:visibility="visible" />

            </LinearLayout>


            <androidx.swiperefreshlayout.widget.SwipeRefreshLayout
                android:id="@+id/pullToRefreshApproved"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/dateSelectionLayout">

                <androidx.recyclerview.widget.RecyclerView
                    android:id="@+id/approved_list_recyclerview"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/value_5dp"
                    android:layout_marginTop="@dimen/value_5dp"
                    android:layout_marginEnd="@dimen/value_5dp" />


            </androidx.swiperefreshlayout.widget.SwipeRefreshLayout>


            <androidx.swiperefreshlayout.widget.SwipeRefreshLayout
                android:id="@+id/pullToRefreshPending"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/dateSelectionLayout">

                <androidx.recyclerview.widget.RecyclerView
                    android:id="@+id/pending_list_recyclerview"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/value_5dp"
                    android:layout_marginTop="@dimen/value_9dp"
                    android:layout_marginEnd="@dimen/value_5dp"
                    android:visibility="gone" />


            </androidx.swiperefreshlayout.widget.SwipeRefreshLayout>



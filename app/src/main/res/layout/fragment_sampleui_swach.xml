<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/light_blue"
        tools:context=".ui.home.sampleui.swachuploadmodule.sampleswachui.SampleUiSwach">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">


            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_marginStart="@dimen/value_20dp"
                android:layout_marginTop="@dimen/value_10dp"
                android:orientation="horizontal">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <ImageView
                        android:layout_gravity="center"
                        android:layout_width="@dimen/value_25dp"
                        android:layout_height="@dimen/value_25dp"
                        android:src="@drawable/storehome"
                        android:contentDescription="@string/app_name" />

                    <LinearLayout
                        android:layout_marginStart="@dimen/value_8dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Store ID"
                            android:textStyle="bold"
                            android:textColor="@color/heading_blue"
                            android:textSize="@dimen/text_12sp" />

                        <TextView
                            android:id="@+id/store_id"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textColor="@color/black"
                            android:textSize="@dimen/text_12sp"
                            android:textStyle="bold" />


                    </LinearLayout>
                </LinearLayout>


                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/value_25dp"
                    android:orientation="horizontal">

                    <ImageView
                        android:layout_gravity="center"
                        android:layout_width="@dimen/text_22sp"
                        android:layout_height="@dimen/text_22sp"
                        android:src="@drawable/person_1" />

                    <LinearLayout
                        android:layout_marginStart="@dimen/value_6dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="User ID"
                            android:textColor="@color/heading_blue"
                            android:textSize="@dimen/text_12sp" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/user_id"
                            android:textColor="@color/black"
                            android:textSize="@dimen/text_12sp"
                            android:textStyle="bold" />


                    </LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/value_25dp"
                    android:orientation="horizontal">

                    <ImageView
                        android:layout_width="@dimen/text_22sp"
                        android:layout_height="@dimen/text_22sp"
                        android:layout_gravity="center"
                        android:src="@drawable/calendar" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/value_6dp"
                        android:orientation="vertical">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Last Uploaded"
                            android:textColor="@color/heading_blue"
                            android:textSize="@dimen/text_12sp" />

                        <TextView
                            android:id="@+id/uploaded_on"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textColor="@color/black"
                            android:textSize="@dimen/text_12sp"
                            android:textStyle="bold" />


                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_margin="@dimen/value_10dp"

                android:background="@drawable/blue_rectangle"
                android:gravity="center"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/todaysUpdateLayout"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/text_12sp"
                    android:text="Today's Update"
                    android:textAlignment="center"
                    android:textColor="@color/white"
                    android:textSize="@dimen/text_17sp"
                    android:textStyle="bold" />

                <TextView
                    android:layout_marginBottom="@dimen/value_5dp"
                    android:id="@+id/todaysDateLayout"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/value_7dp"
                    android:textAlignment="center"
                    android:textColor="@color/white"
                        android:textSize="@dimen/text_15sp"
                    android:textStyle="bold" />

                <LinearLayout
                    android:layout_marginBottom="@dimen/value_10dp"
                    android:id="@+id/uploadNowLayout"
                    android:visibility="gone"
                    android:layout_width="@dimen/value_130dp"
                    android:layout_height="wrap_content"
                    android:padding="@dimen/value_5dp"
                    android:layout_marginTop="@dimen/value_7dp"
                    android:background="@drawable/green_background">

                    <TextView
                        android:visibility="visible"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="Upload Now"
                        android:id="@+id/uploadNowbtnn"
                        android:textAlignment="center"
                        android:textColor="@color/white"
                        android:textSize="@dimen/text_16sp"
                        android:textStyle="bold" />


                </LinearLayout>
<!-- android:background="@drawable/grey_backgroundup"-->
                <LinearLayout
                    android:layout_marginBottom="@dimen/value_10dp"
                    android:id="@+id/uploadNowGrey"
                    android:visibility="gone"
                    android:orientation="vertical"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:padding="@dimen/value_5dp"
                    android:layout_marginTop="@dimen/value_7dp">

                    <TextView
                        android:visibility="visible"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="Next Upload"
                        android:textAlignment="center"
                        android:textColor="@color/white"
                        android:textSize="@dimen/text_17sp"
                        android:textStyle="bold" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/nextUploadDate"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/value_7dp"
                            android:textAlignment="center"
                            android:textColor="@color/white"
                            android:textSize="15sp"
                            android:textStyle="bold"
                            android:visibility="visible" />


                    </LinearLayout>



                </LinearLayout>

                <LinearLayout
                    android:orientation="vertical"
                    android:layout_marginBottom="@dimen/value_10dp"
                    android:visibility="gone"
                    android:id="@+id/uploadOnLayout"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:padding="@dimen/six6_sp"
                    android:layout_marginTop="@dimen/value_7dp"
                  >

                    <TextView
                        android:visibility="visible"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="Next Upload on "
                        android:textAlignment="center"
                        android:textColor="@color/white"
                        android:textSize="@dimen/text_16sp"
                        android:textStyle="bold" />

                    <TextView
                        android:layout_marginTop="@dimen/value_7dp"
                        android:id="@+id/dayoftheweekLayout"
                        android:visibility="visible"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textAlignment="center"
                        android:textColor="@color/white"
                        android:textSize="@dimen/text_15sp"
                        android:textStyle="bold" />


                </LinearLayout>

                <LinearLayout
                    android:layout_marginBottom="@dimen/value_10dp"
                    android:visibility="gone"
                    android:id="@+id/alreadyUploadedlayout"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:orientation="horizontal"
                    android:layout_width="220dp"
                    android:layout_height="wrap_content"
                    android:padding="@dimen/six6_sp"
                    android:layout_marginTop="@dimen/value_7dp"
                    android:background="@drawable/ash_rounded_corners">

                    <TextView
                        android:visibility="visible"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Next Upload on "
                        android:textAlignment="center"
                        android:textColor="@color/white"
                        android:textSize="@dimen/text_16sp"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/upload_day"
                        android:visibility="visible"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textAlignment="center"
                        android:textColor="@color/white"
                        android:textSize="@dimen/text_15sp"
                        android:textStyle="bold" />


                </LinearLayout>




            </LinearLayout>

            <androidx.swiperefreshlayout.widget.SwipeRefreshLayout
                android:id="@+id/pullToRefresh"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/dateSelectionLayout">

                <androidx.recyclerview.widget.RecyclerView
                    android:id="@+id/imageRecyclerView"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/value_5dp"
                    android:layout_marginEnd="@dimen/value_5dp"
                    android:orientation="horizontal" />

            </androidx.swiperefreshlayout.widget.SwipeRefreshLayout>






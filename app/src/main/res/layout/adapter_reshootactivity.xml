<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/light_blue">


    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/value_5dp"
            android:orientation="vertical">


            <LinearLayout
                android:weightSum="1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_weight="0.8"
                    android:layout_marginStart="@dimen/value_5dp"
                    android:layout_height="wrap_content">


                    <TextView
                        android:maxLines="2"
                        android:id="@+id/categoryNameSwachRes"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:textColor="@color/black"
                        android:textSize="@dimen/text_14sp"
                        android:textStyle="bold" />
                </LinearLayout>


                <LinearLayout
                    android:id="@+id/reshoot_layoutRes"
                    android:layout_marginEnd="@dimen/value_5dp"
                    android:layout_width="0dp"
                    android:layout_weight="0.2"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:visibility="gone">


                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:padding="2sp"
                        android:text="Re-Shoot"
                        android:layout_gravity="end"
                        android:textAlignment="center"
                        android:textColor="@color/color_red"
                        android:textSize="@dimen/text_14sp"
                        android:textStyle="bold"
                        tools:ignore="RtlCompat" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/partially_approved_res"
                    android:layout_marginEnd="@dimen/value_5dp"
                    android:layout_width="0dp"
                    android:layout_weight="0.2"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:visibility="gone">


                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:padding="2sp"
                        android:text="Partial"
                        android:textColor="@color/color_red"
                        android:textSize="@dimen/text_14sp"
                        android:layout_gravity="end"
                        android:textStyle="bold"
                        tools:ignore="RtlCompat" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/approved_layoutRes"
                    android:layout_marginEnd="@dimen/value_5dp"
                    android:layout_width="0dp"
                    android:layout_weight="0.2"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:visibility="gone">


                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="end"
                        android:padding="2sp"
                        android:text="Approved"
                        android:textAlignment="center"
                        android:textColor="@color/greenn"
                        android:textSize="@dimen/text_14sp"
                        android:layout_gravity="end"
                        android:textStyle="bold"
                        tools:ignore="RtlCompat" />


                </LinearLayout>

                <LinearLayout

                    android:id="@+id/pending_layoutRes"
                    android:layout_marginEnd="@dimen/value_5dp"
                    android:layout_width="0dp"
                    android:layout_weight="0.2"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:visibility="gone">


                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="end"
                        android:padding="2sp"
                        android:text="Pending"
                        android:textAlignment="center"
                        android:textColor="@color/material_amber_accent_700"
                        android:textSize="@dimen/text_14sp"
                        android:layout_gravity="end"
                        android:textStyle="bold"
                        tools:ignore="RtlCompat" />
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">


                <androidx.recyclerview.widget.RecyclerView
                    android:id="@+id/uploadlayoutrecyclerviewRes"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:visibility="visible" />


            </LinearLayout>


        </LinearLayout>


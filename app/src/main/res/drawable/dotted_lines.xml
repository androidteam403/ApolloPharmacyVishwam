<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:top="0dp"
        android:bottom="0dp"
        android:left="0dp">

        <shape xmlns:android="http://schemas.android.com/apk/res/android"
            android:shape="rectangle">
            <corners android:radius="@dimen/value_5dp"/>
            <gradient android:startColor="@color/upload_photo_bg" android:centerColor="@color/upload_photo_bg" android:endColor="@color/upload_photo_bg"/>
            <stroke
                android:width="1dp"
                android:color="@color/grey"
                android:dashGap="5dp"
                android:dashWidth="7dp" />
        </shape>
    </item>

</layer-list>